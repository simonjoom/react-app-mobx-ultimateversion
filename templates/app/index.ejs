								return 'top';
							}else{
								return 'right';
							}
						}
					}).mouseenter(function(e) {
						if(!$(this).hasClass("showpop")||$(this).hasClass("hidepop"))
						{
							$(this).removeClass("hidepop").addClass("showpop");
							$(this).popover('show');
							$(this).css('color', '#985DCC');

						} else{
							$(this).removeClass("showpop").addClass("hidepop");
